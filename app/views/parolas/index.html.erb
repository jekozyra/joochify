<h1>Listing parolas</h1>

<table class="table">
  <thead>
    <tr>
      <th>Output</th>
      <th>Input</th>
			<th>Part of Speech</th>
			<th>Etymology Language</th>
			<th>Definition</th>
			<th>Tags</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @parolas.each do |parola| %>
      <tr>
        <td><%= parola.output %></td>
        <td><%= parola.input %></td>
				<td><%= parola.part_of_speech %></td>
				<td><%= parola.etymology_language %></td>
				<td><%= parola.definition %></td>
				<td><%= parola.tag_list %></td>
        <td><%= link_to 'Show', parola %></td>
        <td><%= link_to 'Edit', edit_parola_path(parola) %></td>
        <td><%= link_to 'Destroy', parola, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parola', new_parola_path %>
